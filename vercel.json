{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/two-factor-authentication-login", "dest": "/two-factor-authentication-login.html" },
    { "src": "/two-factor-authentication", "dest": "/two-factor-authentication.html" },
    { "src": "/messages", "dest": "/messages.html" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
